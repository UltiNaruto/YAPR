# Top most editorconfig
root = true

# We need Unix-style endings for C# files
# This is because we decompile code with UTMT, which uses Unix file endings
# Since we have code-replace in code files, Windows File endings causes runtime errors
[*.cs]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
charset = utf-8
trim_trailing_whitespace = true
